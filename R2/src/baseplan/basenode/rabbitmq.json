{
  "rabbitmq": {
    "vhosts": [
      "/"
    ],
    "users": {
      "${rabbitmq_username}": {
        "password": "${rabbitmq_password}",
        "tag": "administrator",
        "permissions": {
          "/": {
            "conf": ".*",
            "write": ".*",
            "read": ".*"
          }
        }
      }
    }
  },
  "dtdata": {
    "retrieve_method": "archive",
    "archive_url": "${dtdata_archive_url}",
    "chef_debug_level": "debug"
  },
  "recipes": [
    "sshkeys",
    "rabbitmq",
    "rabbitmq::management"
  ]
}
